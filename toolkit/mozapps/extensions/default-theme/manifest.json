{
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "id": "dynamic@themes.dothq.co"
    }
  },
  "name": "Automatic",
  "description": "A theme with an automatic color scheme.",
  "author": "Dot HQ",
  "version": "1.1",
  "icons": {
    "32": "icon.svg"
  },
  "theme": {
    "colors": {
      "tab_background_text": "rgb(24, 25, 26)",
      "icons": "rgb(24, 25, 26, 0.7)",
